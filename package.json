{
  "name": "fetchyeah",
  "description": "Miniscule JSON fetch wrapper library.",
  "version": "5.0.3",
  "author": "Zac Anger (https://zacanger.com)",
  "funding": {
    "type": "ko-fi",
    "url": "https://ko-fi.com/zacanger"
  },
  "license": "MIT",
  "main": "lib/index.cjs.js",
  "module": "lib/index.js",
  "scripts": {
    "build": "run-s -s build:clean build:rollup",
    "build:clean": "rm -rf lib && mkdir -p lib",
    "build:rollup": "rollup -c",
    "cover": "npm run test:jest -- --coverage --no-cache",
    "format": "sortpack --write \"src/*.ts\"",
    "prebuild": "run-s -s format test:build",
    "preversion": "npm run build",
    "test": "run-s -s test:ts test:lint test:jest",
    "test:build": "run-s -s test:ts test:lint cover",
    "test:jest": "jest",
    "test:lint": "ts-standard src/*.ts --fix",
    "test:ts": "tsc --noEmit"
  },
  "homepage": "https://github.com/zacanger/fetchyeah#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zacanger/fetchyeah.git"
  },
  "bugs": "https://github.com/zacanger/fetchyeah/issues",
  "keywords": [
    "browser",
    "fetch",
    "json",
    "node",
    "request",
    "xhr"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "25.0.7",
    "@rollup/plugin-node-resolve": "15.2.3",
    "@rollup/plugin-terser": "0.4.4",
    "@rollup/plugin-typescript": "11.1.6",
    "@types/jest": "29.5.12",
    "@types/node": "20.12.7",
    "isomorphic-fetch": "3.0.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "rollup": "4.16.4",
    "sortpack": "2.4.0",
    "ts-jest": "29.1.2",
    "ts-standard": "12.0.2",
    "tslib": "2.6.2",
    "typescript": "5.4.5",
    "uglify-es": "3.3.10"
  },
  "types": "./lib/index.d.ts",
  "jest": {
    "collectCoverageFrom": [
      "src/index.ts"
    ],
    "coverageReporters": [
      "json-summary",
      "lcov",
      "text",
      "json"
    ],
    "moduleFileExtensions": [
      "js",
      "ts",
      "json"
    ],
    "modulePaths": [
      "<rootDir>/src/"
    ],
    "setupFiles": [
      "./config/jest-setup.js"
    ],
    "testEnvironment": "jsdom",
    "testEnvironmentOptions": {
      "url": "http://localhost"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/(lib|node_modules)/"
    ],
    "testRegex": "\\.test\\.ts$",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    }
  }
}
